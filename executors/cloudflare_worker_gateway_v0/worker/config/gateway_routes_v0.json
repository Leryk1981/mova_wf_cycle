{
  "version": "v0",
  "routes": [
    {
      "domain": "demo.local",
      "action": "echo",
      "mode": "service",
      "service_binding": "SERVICE_ECHO",
      "path": "/echo",
      "timeout_ms": 1500,
      "max_response_bytes": 8192
    },
    {
      "domain": "demo.local",
      "action": "slow",
      "mode": "service",
      "service_binding": "SERVICE_SLOW",
      "path": "/slow",
      "timeout_ms": 75,
      "max_response_bytes": 4096
    },
    {
      "domain": "remote.example",
      "action": "status",
      "mode": "https",
      "url": "https://remote.example/gateway/status",
      "env_url_key": "GATEWAY_REMOTE_STATUS_URL",
      "hmac_secret_env": "GATEWAY_HMAC_SECRET",
      "timeout_ms": 1200,
      "max_response_bytes": 4096
    },
    {
      "domain": "mova.domain.test",
      "action": "test",
      "mode": "https",
      "url": "https://mova-cloudflare-worker-v1.s-myasoedov81.workers.dev",
      "hmac_secret_env": "GW_SECRET_KEY",
      "timeout_ms": 2000,
      "max_response_bytes": 16384
    },
    {
      "domain": "mova.domain.test",
      "action": "probe",
      "mode": "https",
      "url": "https://mova-cloudflare-worker-v1.s-myasoedov81.workers.dev",
      "path": "/__gw_probe",
      "hmac_secret_env": "GW_SECRET_KEY",
      "timeout_ms": 2000,
      "max_response_bytes": 16384
    }
  ]
}
